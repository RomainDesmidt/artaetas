    <% if Rails.env.development? %>
      <%= favicon_link_tag asset_path('AE_blanc.png'), :rel => 'icon', :type =>  'image/png' %>
    <% else %>
      <%= favicon_link_tag asset_path('artaetas_noir_fav.ico'), :rel => 'icon', :type =>  'image/x-icon' %>
    <% end %>
    
    
    
  </head>
  <body>
    <%= render 'shared/navbar' %>
    <%= render 'shared/flashes' %>
    <%= yield %>
    <%= javascript_include_tag 'application' %>
